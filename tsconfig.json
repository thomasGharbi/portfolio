{
    "compilerOptions": {
        "target": "es6",
        "module": "esnext",
        "jsx": "react-jsx",
        "moduleResolution": "node",
        "allowJs": true,
        "checkJs": false,
        "strict": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "skipLibCheck": true
    },
    "include": ["assets"]
}
